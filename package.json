{
  "name": "@wfcd/mod-generator",
  "public": "true",
  "version": "0.0.1",
  "description": "",
  "author": "SlayerOrnstein",
  "type": "module",
  "main": "./dist/generator.mjs",
  "types": "./dist/generator.d.mts",
  "files": [
    "dist/",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsdown",
    "build:docs": "typedoc",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "test": "c8 mocha tests/*.spec.ts",
    "prepare": "husky"
  },
  "license": "MIT",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.94",
    "warframe-items": "^1.1269.87"
  },
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@commitlint/types": "^20.4.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.3.0",
    "@wfcd/eslint-config": "^1.6.4",
    "c8": "^10.1.3",
    "chai": "^6.2.2",
    "coveralls": "^3.1.1",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "mocha": "^11.7.5",
    "tsdown": "^0.20.3",
    "tsx": "4.21.0",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-transform-class-properties",
      "@babel/plugin-transform-private-methods"
    ]
  },
  "mocha": {
    "timeout": 20000,
    "exit": true,
    "node-option": [
      "import=tsx"
    ]
  },
  "c8": {
    "reporter": [
      "lcov",
      "text"
    ],
    "skip-full": true
  },
  "peerDependencies": {
    "warframe-items": "^1.1269.87"
  }
}
